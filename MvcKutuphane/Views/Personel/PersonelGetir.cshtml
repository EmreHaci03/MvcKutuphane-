@model MvcKutuphane.Models.Entity.TBL_PERSONEL
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}



@using (Html.BeginForm("PersonelGüncelle", "Personel", FormMethod.Post))
{
    <div style="margin-top:15px"></div>
    <div class="form-group">
        @Html.LabelFor(p => p.ID)
        @Html.TextBoxFor(p => p.ID, new { @class = "form-control" })
        <div style="margin-top:15px"></div>
        @Html.LabelFor(p => p.PersonelAdSoyad)
        @Html.TextBoxFor(p => p.PersonelAdSoyad, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>

}
